<div class="container">
	<div class="row">
		<div class="col-xs-12 course-list">
			<%	for (var key in config.courses) { 
				course = config.courses[key]; %>
				<%- partial("_partial/course-item", {slug: key, title: course.title, description: course.description , image: course.image}) %>
			<% } %>
		</div>
	</div>
</div>